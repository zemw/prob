N <- 600  # total simulation time
p <- .1   # prob of occurrence per unit time
set.seed(0)
# I[t] = 1 if an event occurs in time t
I <- 1* (runif(N) < p)
# plot the random occurrence
plot(1:N, I, type = "h")
# inter-arrival time
T <- diff( (1:N)[I==1] )
# distribution if waiting time
hist(T, prob=TRUE)
